<%= form_for @annotation do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :annotatable_id %><br />
    <%= f.text_field :annotatable_id %>
  </p>
  <p>
    <%= f.label :annotatable_type %><br />
    <%= f.text_field :annotatable_type %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p><%= f.submit %></p>
<% end %>
