<%= form_for @task do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :assigned_to %><br />
    <%= f.text_field :assigned_to %>
  </p>
  <p>
    <%= f.label :completed_by %><br />
    <%= f.text_field :completed_by %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :subject_id %><br />
    <%= f.text_field :subject_id %>
  </p>
  <p>
    <%= f.label :subject_type %><br />
    <%= f.text_field :subject_type %>
  </p>
  <p>
    <%= f.label :due_at %><br />
    <%= f.datetime_select :due_at %>
  </p>
  <p>
    <%= f.label :completed_at %><br />
    <%= f.datetime_select :completed_at %>
  </p>
  <p>
    <%= f.label :deleted_at %><br />
    <%= f.datetime_select :deleted_at %>
  </p>
  <p><%= f.submit %></p>
<% end %>
