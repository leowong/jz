<!DOCTYPE html>
<html>
<head>
  <title>Jzcrm</title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults, "nested_form" %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div id="wrapper">
    <div class="container">
      <table class="content">
	<tr>
	  <td class="main">
	    <div class="col">
	      <%= yield %>
	    </div>
	  </td>
	  <td class="side">
	    <div class="col">
	      <%= yield :side %>
	    </div>
	  </td>
	</tr>
      </table>
    </div>
  </div>
</body>
</html>
